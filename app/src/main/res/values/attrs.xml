<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberPickerPreference">
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="wrapSelectorWheel" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ListSwipeItem">
        <attr name="swipeViewId" format="integer"/>
        <attr name="leftViewId" format="integer"/>
        <attr name="rightViewId" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BoardView">
        <attr name="boardEdges" format="dimension"/>
        <attr name="columnSpacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="FixedRatioButton">
        <attr name="buttonIcon" format="reference"/>
        <attr name="buttonIconDisabled" format="reference"/>
        <attr name="buttonIconSize" format="float"/>
    </declare-styleable>

</resources>